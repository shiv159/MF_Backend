	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Register User",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Generate unique email for testing",
									"const randomId = Math.random().toString(36).substring(2, 8);",
									"pm.environment.set('test_email', `testuser${randomId}@example.com`);"
								]
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 200', function () {",
									"  pm.response.to.have.status(200);",
									"});",
									"",
									"const response = pm.response.json();",
									"pm.test('Response has required fields', function () {",
									"  pm.expect(response).to.have.property('userId');",
									"  pm.expect(response).to.have.property('email');",
									"});",
									"",
									"pm.test('Save user ID and email for next requests', function () {",
									"  if (response.userId) {",
									"    pm.environment.set('registered_user_id', response.userId);",
									"    pm.environment.set('registered_email', pm.environment.get('test_email'));",
									"  }",
									"});"
								]
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"{{test_email}}\",\n  \"password\": \"SecurePassword123\",\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/auth/register",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"v1",
								"auth",
								"register"
							]
						}
					},
					"response": []
				},
				{
					"name": "Login",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Use the registered email or default test email",
									"const email = pm.environment.get('registered_email') || 'testuser@example.com';",
									"const password = 'SecurePassword123';",
									"",
									"pm.environment.set('login_email', email);",
									"pm.environment.set('login_password', password);"
								]
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 200', function () {",
									"  pm.response.to.have.status(200);",
									"});",
									"",
									"const response = pm.response.json();",
									"",
									"pm.test('Response has access_token', function () {",
									"  pm.expect(response).to.have.property('accessToken');",
									"});",
									"",
									"pm.test('Response has userId', function () {",
									"  pm.expect(response).to.have.property('userId');",
									"});",
									"",
									"if (response.accessToken) {",
									"  pm.environment.set('access_token', response.accessToken);",
									"  pm.environment.set('user_id', response.userId);",
									"  console.log('Access token saved:', response.accessToken.substring(0, 20) + '...');",
									"}"
								]
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"{{login_email}}\",\n  \"password\": \"{{login_password}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/auth/login",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"v1",
								"auth",
								"login"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "Upload Portfolio",
			"item": [
				{
					"name": "Upload Excel File (Conservative)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 200', function () {",
									"  pm.response.to.have.status(200);",
									"});",
									"",
									"const response = pm.response.json();",
									"",
									"pm.test('Response has uploadId', function () {",
									"  pm.expect(response).to.have.property('uploadId');",
									"});",
									"",
									"pm.test('Response has status', function () {",
									"  pm.expect(response).to.have.property('status');",
									"});",
									"",
									"pm.test('uploadId is valid UUID', function () {",
									"  const uuidRegex = /^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$/i;",
									"  pm.expect(response.uploadId).to.match(uuidRegex);",
									"});",
									"",
									"pm.test('Status is valid', function () {",
									"  const validStatuses = ['PENDING', 'PARSING', 'ENRICHING', 'COMPLETED', 'FAILED'];",
									"  pm.expect(validStatuses).to.include(response.status.toUpperCase());",
									"});",
									"",
									"if (response.uploadId) {",
									"  pm.environment.set('upload_id', response.uploadId);",
									"  console.log('Upload ID saved:', response.uploadId);",
									"}"
								]
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{access_token}}"
							}
						],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "userId",
									"value": "{{user_id}}",
									"type": "text"
								},
								{
									"key": "portfolioName",
									"value": "Conservative Portfolio",
									"type": "text"
								},
								{
									"key": "file",
									"type": "file",
									"src": "/path/to/your/conservative_portfolio.xlsx"
								}
							]
						},
						"url": {
							"raw": "{{base_url}}/api/onboarding/uploads",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"onboarding",
								"uploads"
							]
						}
					},
					"response": []
				},
